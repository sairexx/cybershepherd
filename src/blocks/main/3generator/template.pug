        h2.generator__head-title генератор импульсов «Статик-3м»
      .generator__list
        .generator__item
          .generator__item-preview
            img(src=`${icnUrl}/generator-star.svg` alt="").generator__item-icon.generator__item-icon_star
            img(src=`${icnUrl}/generator-cow.svg` alt="").generator__item-icon.generator__item-icon_cow
